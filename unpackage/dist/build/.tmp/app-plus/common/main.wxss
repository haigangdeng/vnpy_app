.grace-comments .body{width:100%;}
.grace-comments .header{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; line-height:1.5em; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between; margin-top:-4rpx;}
