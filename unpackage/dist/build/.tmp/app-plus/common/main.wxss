.grace-scroll-do > scroll-view:last-child{border:none;}
.grace-scroll-do .items{width:700rpx; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; overflow:hidden; height:80rpx; margin:20rpx 0; -webkit-box-align:flex-center; -webkit-align-items:flex-center; align-items:flex-center;}
