.grace-form .grace-items .other{width:100%; line-height:80rpx; display:block; overflow:hidden; -webkit-flex-shrink:1; flex-shrink:1; text-align:right;}
.grace-form picker{width:100%; height:80rpx; line-height:80rpx; color:#333333; background:none; border:0; text-align:right;}
.grace-form picker text{color:#333; font-size:28rpx;}
