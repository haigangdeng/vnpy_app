.grace-tab-title{white-space:nowrap; height:104rpx; text-align:center; background:#FFFFFF;}
.grace-tab-title view{width:auto; padding:0 28rpx; margin:0 16rpx; height:80rpx; line-height:80rpx; display: inline-block; text-align:center; border-bottom:4rpx solid #FFFFFF; overflow:hidden;}
.grace-tab-title view:first-child{margin-left: 0;}
.grace-tab-title view:last-child{margin-right: 0;}
.grace-tab-current{border-bottom:4rpx solid #3688FF !important; color:#3688FF; font-weight:700;}
.grace-tab-swiper{width:100%; height:350rpx; overflow:hidden; padding:0;}
