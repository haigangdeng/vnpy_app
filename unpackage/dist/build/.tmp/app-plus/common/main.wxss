.grace-form textarea{width:100%; height:120rpx; line-height:1.2em; background:none; border:0; font-size:28rpx; color:#333333; text-align:left;}
@font-face{font-family:"grace-iconfonts"; src:url('data:application/x-font-woff;charset=utf-8;base64,');}
.grace-label-x{width:100%; padding:12rpx 0; -webkit-flex-shrink:1; flex-shrink:1; margin-left:40rpx;}
