.grace-scroll-do .items > .image > image{width:80rpx; height:80rpx; border-radius:80rpx;}
.grace-scroll-do .items > .contents{margin-right:30rpx; -webkit-box-flex:1; -webkit-flex:auto; flex:auto;}
.grace-scroll-do .items > .contents > .title{display:-webkit-box;display:-webkit-flex;display:flex; -webkit-flex-wrap:nowrap; flex-wrap:nowrap; -webkit-box-pack:justify; -webkit-justify-content:space-between; justify-content:space-between; font-size:28rpx; color:#333333;}
