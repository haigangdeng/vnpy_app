/* 查库存界面的输入框和查询框美化 */
.search-input {
	/* margin-left: 50upx; */
	height: 28px;
	width: 60%;
	padding: 10rpx 10rpx 10rpx 10rpx;
	border: #007aff solid 2rpx;
	border-top-left-radius: 15rpx;
	border-bottom-left-radius: 15rpx;
	line-height: 20px !important;
}
.search-button {
	/* margin-right: 50upx; */
	height: 28px;
	width: 35%;
	background: #007aff;
	padding: 10rpx 10rpx 10rpx 10rpx;
	border: #007aff solid 2rpx;
	border-top-right-radius: 15rpx;
	border-bottom-right-radius: 15rpx;
}
.search-button button:after {
	border: none;
	background: none;
}
.btn-search {
	display: block;
	height: 100%;
	width: 100%;
	line-height: 20px;
	opacity: 1;
	text-align: left;
	vertical-align: middle;
	border-radius: 0rpx 0rpx 0rpx 0rpx;
	background: none;
	border: 0;
	padding: 10rpx 10rpx 10rpx 10rpx;
	font-size: 18px;
	overflow: hidden;
	color: #FFFFFF;
	font-size: 16px;
	font-weight: bold;
}
